<?xml version="1.0" encoding="utf-8"?>

<s:SparkSkin xmlns:fx="http://ns.adobe.com/mxml/2009"
             xmlns:s="library://ns.adobe.com/flex/spark"
             minHeight="10">

    <fx:Metadata>
		<![CDATA[
        [HostComponent("spark.components.Button")]
        ]]>
	</fx:Metadata>

    <s:states>
        <s:State name="up" />
        <s:State name="over" />
        <s:State name="down" />
        <s:State name="disabled" />
    </s:states>

    <s:Rect width="100%"
            height="100%"
            radiusX="{height/2}"
            radiusY="{height/2}">
        <s:fill>
            <s:SolidColor color="{getStyle('track_color')}" />
        </s:fill>

    </s:Rect>

</s:SparkSkin>